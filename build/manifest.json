{
  "manifest_version": 2,
  "homepage_url": "https://github.com/akilism/new-tab",
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  },
  "permissions": [
    "background",
    "tabs"
  ],
  "icons": {
    "16": "$assets/webpack-16.png",
    "32": "$assets/webpack-32.png",
    "128": "$assets/webpack-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://localhost:3001; object-src 'self'; ",
  "name": "WebpackChromeExtension",
  "description": "Bootstrap playground for developing Chrome extensions with all new features like Webpack, Babel, React and almost everything you want.",
  "version": "0.3.1"
}